<?xml version="1.0" encoding="UTF-8"?> 
<feature id="org.tmatesoft.javasvn.subclipse"
	version="%javasvn.version%"
	label="JavaSVN Subclipse Extenstion"
	provider-name="TMate Software"
	>

   <description url="http://tmate.org/svn/subclipse.html">
     Subclipse extenstion that makes Subclipse use JavaSVN (pure java Subversion client library)
     instead of native javahl bindings.
   </description>

   <copyright url="http://tmatesoft.com/">
   	(c) 2004-2005, TMate Software Ltd.
   </copyright>

   <license url="http://tmate.org/svn/license.html">
   %license
   </license>
   
   <requires>
	<import feature="org.tigris.subversion.subclipse"
		version="%subclipse.version%"
	   	patch="true"
	/>
   </requires>

   <plugin id="org.tmatesoft.javasvn" version="%javasvn.version%" />
   <plugin id="org.tigris.subversion.subclipse.core" version="%subclipse.plugin.version%" />
   
</feature>